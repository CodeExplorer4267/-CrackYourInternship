#include<iostream>
using namespace std;
void reverse(int arr[],int x,int y){
   while(x<=y){
      swap(arr[x],arr[y]);
      x++;
      y--;
   }
}
bool solve(int arr[],int n){
   int x=-1;
   int y=-1;
   for(int i=0;i<n;i++){
      if(arr[i+1]<arr[i]){
         x=i+1;
         break;
      }
   }
   if(x==-1){
      return true;
   }
   for(int i=x;i<n;i++){
      if(arr[i]<arr[i+1]){
         y=i;
         break;
      }
   }
   if(y==-1){
      y=n-1;
   }
   reverse(arr,x,y);
   for(int i=0;i<n-1;i++){
         if(arr[i]>arr[i+1]){
            return false;
         }
   }
   return true;
}
int main(){
   int arr[8]={1,2,5,4,3,6,7,8};
   bool ans=solve(arr,8);
   if(ans==true){
      cout<<"Present";
   }
   else{
      cout<<"Not present";
   }
   return 0;
}
